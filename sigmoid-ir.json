{
    "resourceSpecId": "11111111-1111-4111-8111-111111111111",
    "stableId": "apigateway",
    "displayName": "API Gateway",
    "category": "network",
    "iconUrl": "https://cdn.sigmoid.us/icons/aws/full-size/svg/networking/api-gateway.svg",
    "resourceType": "api.gateway",
    "vpcDependency": "optional",
    "status": "draft",
    "terraform": {
        "module": {
            "source": "github.com/sigmoid-hq/terraform-aws-apigateway",
            "ref": "main"
        }
    },
    "pricing": {
        "awsPricingLookup": {
            "serviceCode": "AmazonApiGateway",
            "unit": "per_million",
            "quantityProperties": ["monthly_requests"],
            "quantityDefault": 0,
            "filters": [
                { "field": "location", "value": "${location}" },
                { "field": "productFamily", "defaultValue": "API Calls", "required": false }
            ]
        },
        "usageProfiles": {
            "default": "medium",
            "low": {
                "monthly_requests": 1000000
            },
            "medium": {
                "monthly_requests": 20000000
            },
            "high": {
                "monthly_requests": 200000000
            },
            "xhigh": {
                "monthly_requests": 1000000000
            }
        }
    },
    "position": { "x": 0, "y": 0 }
}
